generator client {
  provider  = "prisma-client"
  engineType = "client"
  output    = "./generated"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cache {
  key       String   @id
  data      Json
  expireAt  DateTime
  createdAt DateTime @default(now())

  @@index([expireAt])
}
